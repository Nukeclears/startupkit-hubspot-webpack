{
  "name": "startupkit-hubspot-webpack",
  "version": "1.0.0",
  "author": "Thomas van Deursen",
  "license": "MIT",
  "main": "Index.js",
  "private": true,
  "scripts": {
    "webpack::build": "cross-env NODE_ENV=production webpack --config ./.config/webpack.config.js",
    "webpack::debugging": "webpack --watch --mode development --config ./.config/webpack.config.js",
    "upgrade": "yarn upgrade-interactive"
  },
  "browserslist": [
    "defaults"
  ],
  "cssnano": {
    "preset": "default"
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "indentation": 4,
      "selector-max-compound-selectors": null,
      "scss/selector-no-redundant-nesting-selector": null,
      "selector-class-pattern": null,
      "unit-disallowed-list": [
        "px",
        "em"
      ],
      "selector-max-id": 1,
      "selector-no-qualifying-type": null,
      "block-no-empty": [
        true,
        {
          "severity": "warning"
        }
      ],
      "scss/at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "extends",
            "apply",
            "tailwind",
            "layer",
            "components",
            "utilities",
            "screen"
          ]
        }
      ]
    }
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "strict": [
        "error",
        "global"
      ],
      "no-unused-vars": "warn",
      "curly": "warn",
      "indent": [
        "warn",
        4
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@alpinejs/collapse": "^3.10.5",
    "@alpinejs/focus": "^3.10.5",
    "@alpinejs/intersect": "^3.10.5",
    "@alpinejs/mask": "^3.10.5",
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vimeo/player": "^2.20.1",
    "alpinejs": "3.10.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "blaze-slider": "^1.9.3",
    "browserlist": "^1.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.43.0",
    "eslint-webpack-plugin": "^4.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "simple-parallax-js": "^5.6.2",
    "stylelint": "^15.9.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "resolutions": {
    "@types/eslint": "^7.29.0",
    "autoprefixer": "10.4.5"
  },
  "packageManager": "yarn@3.6.0"
}
